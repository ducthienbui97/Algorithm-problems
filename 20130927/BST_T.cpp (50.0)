#include <bits/stdc++.h>
using namespace std;
#define elif else if
struct node{
  node *l,*r;
  int val;
};
int cnt;
node *root;

void insert(int val){
    if(root==NULL){
        root=new node;
        root->l=root->r=NULL;
        root->val=val;
        return;
    }
    node *p=NULL;
    node *now=root;
    while(now!=NULL){
        cnt++;
        p=now;
        if(val>now->val) now=now->r;
        else now=now->l;
    }
    now=new node;
    now->l=now->r=NULL;
    now->val=val;
    if(p->val>val) p->l=now;
    else p->r=now;
}
main(){
    root=NULL;
    int n;
    int p;
    cin>>n;
    for(int i=1;i<=n;i++){
        cin>>p;
        insert(p);
        cout<<cnt<<endl;
    }

    }
/*
8
3
5
1
6
8
7
2
4
*/
